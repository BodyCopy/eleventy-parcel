{% extends "./layout.njk" %}

{% block content %}
  <div class="page-content">
    <div class="container o-richtext">
      <h1>{{ pageTitle }}</h1>
      <ul>
      <li>Created on: {{ page.date.toDateString() }}</li>
      <li>Last updated: {{lastUpdated}}</li>
      </ul>
      {{ content | safe }}
    </div>
  </div>
{% endblock %}